<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<context:annotation-config />
	<bean id="transport" class="com.bridgelabz.Resource.Transport">
		<property name="startingPoint" value="Bangalore"></property>
		<property name="destination" value="Mumbai"></property>
	</bean>

	<bean id="train1" class="com.bridgelabz.Resource.Train">
		<property name="trainName" value="Mumbai Express"></property>
		<property name="cost" value="500"></property>
	</bean>

	<bean id="train2" class="com.bridgelabz.Resource.Train">
		<property name="trainName" value="Kanyakumari Express"></property>
		<property name="cost" value="720"></property>
	</bean>

</beans>