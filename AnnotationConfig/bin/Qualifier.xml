<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-3.0.xsd">
<context:annotation-config/>

<bean id="vehicle" class="com.bridgelabz.Qualifier.Vehicle">
<property name="Company" value="Honda"></property>
<property name="vehicleNum" value="453"></property>
</bean>
<bean id="car1" class="com.bridgelabz.Qualifier.Car">
<property name="carModel" value="Honda Amaze"></property>
<property name="cost" value="750000"></property>
</bean>

<bean id="car2" class="com.bridgelabz.Qualifier.Car">
<property name="carModel" value="Honda City"></property>
<property name="cost" value="950000"></property>
</bean>
</beans>
